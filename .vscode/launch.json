    "version": "0.2.0",
    "configurations": [


        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/GitHubNotifications/Server/bin/Debug/net5.0/GitHubNotifications.Server.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Launch and Debug Hosted Blazor WebAssembly App",
            "type": "blazorwasm",
            "request": "launch",
            "hosted": true,
            "program": "${workspaceFolder}/Server/bin/Debug/net5.0/GitHubNotifications.Server.dll",
            "cwd": "${workspaceFolder}/Server",
            "browser": "edge"
          }
    ]
}